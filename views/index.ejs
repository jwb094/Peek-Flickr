<!DOCTYPE html>
<html>

<head>
    <link type="text/css" rel="stylesheet" href="css/materialize.min.css">
    <link type="text/css" rel="stylesheet" href="css/main.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <title>Flickr Search</title>
</head>

<body>
    <% include ./partials/navigation %>
        <!-- start of search bar and button -->
        <br>
        <!-- SEARCH FORM-->
        <div class="row">

            <form class="col l6">
                <div class="row">
                    <div class="col s6">
                        <input placeholder="Enter Desired Event Location" name="search" id="search" type="text" class="validate" required>
                    </div>
                    <div class="col s6">
                        <button id="submitSearch" class="btn waves-effect waves-light" type="submit" name="action"> <i class="material-icons">search</i> </button></div>
                </div>
            </form>
        </div>


        <!--<div id="map"></div>-->
         <div class="row">
      <!-- GOOGLE EVENTS MAP -->
      <div class="col s12 m6 l6">
        <div id="map"></div>
      </div>

      <div class="col s12 m6 l6">
        <div id="table-scroll">
          <!-- EVENTS TABLE -->
          <table class="bordered">
            <thead>
              <tr>
                <th data-field="Name">Name</th>
              </tr>
            </thead>

            <tbody id="eventData">
            </tbody>
          </table>

     <!-- MODAL -->
                <div id="modal1" class="modal modal-fixed-footer">
                    <div class="modal-content">
                        <h4 id="modalHeader"></h4>
                        <div id="modalText"></div>
                    </div>
                    <div class="modal-footer">
                        <a href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">OK</a>
                    </div>
                </div>
        </div>
      </div>
    </div>





   


       
       <script src="js/jquery-3.1.1.min.js"></script>
       <script src="js/materialize.js"></script>
       <script defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAJ0Xr2B6HTUs5Ji74ml5GL5OiuOENJhnc&callback=initMap"  ></script>
       <script src="js/Photo.js"></script>
       <!-- &callback=initMap-->
</body>

</html>